
@model List<WebApplication4.Models.anjal>

@{
    ViewBag.Title = "Index";
    int sn = 0;
}


<h2>Index</h2>
<style>
    .table{
    background-color: peachpuff;
    }
</style>
<table class="table" border="1">
    <tr>
        <th>SN</th>
        <th>Name</th>
        <th>Address</th>
        <th>Age</th>
        <th>Contact</th>
    </tr>
    @foreach (var data in Model)
    {
<tr>
    <td>@(++sn)</td>
    <td>@(data.name)</td>
    <td>@(data.address)</td>
    <td>@(data.age)</td>
    <td>@(data.contact)</td>
</tr>
    }


</table>
